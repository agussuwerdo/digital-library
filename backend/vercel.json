{
  "version": 2,
  "builds": [
    {
      "src": "handler/handler.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "FRONTEND_URL": "https://digital-library-frontend.vercel.app"
  },
  "github": {
    "enabled": true,
    "silent": true
  },
  "functions": {
    "handler/handler.go": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 